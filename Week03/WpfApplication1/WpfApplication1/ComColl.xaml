<Window x:Class="WpfApplication1.ComColl"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApplication1"
        mc:Ignorable="d"
        Title="ComColl" Height="300" Width="500">
    <Grid Margin="0,0,2,0">
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="21,10,0,0" TextWrapping="Wrap" Text="You are 21 years old." VerticalAlignment="Top" Width="431"/>
        <TextBlock x:Name="textBlock1" HorizontalAlignment="Left" Margin="21,44,0,0" TextWrapping="Wrap" Text="You are currently enrolled in Woolly Community College. The Community College allows you to study whatever subject you would like!  Type the subject you would like to Pursue." VerticalAlignment="Top" Width="422" Height="66"/>
        <Button x:Name="button" Content="Continue" HorizontalAlignment="Left" Margin="46,189,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="46,132,0,0" TextWrapping="Wrap" Text="My Subject" VerticalAlignment="Top" Width="120"/>


    </Grid>
</Window>
